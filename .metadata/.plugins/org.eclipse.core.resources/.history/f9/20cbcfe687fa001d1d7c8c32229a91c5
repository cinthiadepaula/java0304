package atividade;

import java.util.Scanner;

public class atividade01 {
	
	public static void main (String[] args) {
	
		Scanner ler = new Scanner(System.in);
		String nome;
		System.out.print("Digite seu nome: ");
		nome = ler.next();
		
		System.out.println("Seja bem bindo" + nome);
		
	
	
	}
}
